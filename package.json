{
  "name": "kakojs",
  "version": "0.1.74",
  "description": "Lightweight nodejs base minimalist framework to make api easy.",
  "keywords": [
    "Koa",
    "Apollo",
    "Knex",
    "Objection"
  ],
  "author": {
    "name": "Tony T. Sithu",
    "email": "tsithu@gmail.com"
  },
